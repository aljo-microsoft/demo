
<?xml version="1.0" encoding="utf-8" ?>
<ServiceManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="GoApp" Version="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
   <ServiceTypes>
      <StatelessServiceType ServiceTypeName="GoAppServiceType" UseImplicitHost="true"/>
   </ServiceTypes>
   <CodePackage Name="GoCode" Version="1.0.0">
      <EntryPoint>
         <ExeHost>
            <Program>main.exe</Program>
            <WorkingFolder>CodePackage</WorkingFolder>
         </ExeHost>
      </EntryPoint>
	  <EnvironmentVariables>
	    <EnvironmentVariable Name="DATABASE_NAME" Value="" />
	  </EnvironmentVariables>
   </CodePackage>
   <ConfigPackage Name="GoConfig" Version="1.0.0" />
    <Resources>
      <Endpoints>
         <Endpoint Name="GoAppTypeEndpoint" Protocol="http" Type="Input" />
      </Endpoints>
   </Resources>
</ServiceManifest>
